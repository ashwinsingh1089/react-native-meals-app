{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  // "parserOptions": {
  //   "ecmaFeatures": {
  //     "jsx": true
  //   },
  //   "ecmaVersion": 2018,
  //   "sourceType": "module"
  // },
  "plugins": ["react"],
  "rules": {
    "quotes": ["warn", "single"],
    "no-unused-vars": "warn",
    //"quote-props": "warn",
    "no-case-declarations": "off",
    "jsx-quotes": "off",
    "no-use-before-define": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off",
    "comma-dangle": "off",
    "no-mixed-spaces-and-tabs": "off",
    "quote-props": "off"
  }
}
